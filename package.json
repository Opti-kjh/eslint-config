{
  "name": "@deali-web/eslint-config",
  "description": "Dealicious ESLint Config",
  "license": "MIT",
  "author": "Dealicious UI <ui@dealicious.kr>",
  "homepage": "https://github.com/dealicious-inc/eslint-config",
  "repository": {
    "type": "git",
    "url": "https://github.com/deali-web/eslint-config"
  },
  "bugs": {
    "url": "https://github.com/deali-web/eslint-config/issues"
  },
  "version": "0.1.3",
  "main": "index.js",
  "files": [
    "plugins",
    "rules"
  ],
  "scripts": {
    "format": "prettier-package-json --write",
    "lint": "eslint index.js plugins/*.js rules/*.js",
    "prepare": "husky install",
    "release": "release-it",
    "tag": "[ `git rev-parse --abbrev-ref HEAD` = 'main' ] && standard-version --no-verify",
    "test": "yarn lint && yarn format && git diff --quiet"
  },
  "peerDependencies": {
    "babel-eslint": "^10.0.0",
    "eslint": "^7.15.0",
    "eslint-plugin-node": "^11.1.0"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^2.0.1",
    "@typescript-eslint/eslint-plugin": "4.20.0",
    "@typescript-eslint/parser": "4.20.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.23.0",
    "eslint-plugin-jest": "24.3.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-notice": "0.9.10",
    "eslint-plugin-react": "7.23.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "jest": "26.6.3",
    "prettier-package-json": "2.1.3",
    "react": "17.0.2",
    "release-it": "^14.6.1",
    "standard-version": "9.1.1",
    "typescript": "4.2.3"
  },
  "keywords": [
    "config",
    "dealicious",
    "eslint"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "release-it": {
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  }
}
